{
    "name": "pgrimaud/symfony-world-schedule",
    "description": "Get the SymfonyWord Online 2021 schedule from the terminal.",
    "keywords": ["framework", "symfony", "console", "symfony world", "console", "cli"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/pgrimaud/symfony-world-schedule/issues",
        "source": "https://github.com/pgrimaud/symfony-world-schedule"
    },
    "authors": [
        {
            "name": "Pierre Grimaud",
            "email": "grimaud.pierre@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "nesbot/carbon": "^2.46",
        "symfony/console": "5.3.*",
        "symfony/dotenv": "5.3.*",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "5.3.*",
        "symfony/yaml": "5.3.*"
    },
    "require-dev": {
        "symfony/maker-bundle": "^1.30"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd"
        },
        "post-autoload-dump": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.3.*"
        }
    },
    "bin": ["./build/symfony-world"]
}
